{
    "name": "eidsonator/php-design-patterns",
    "license": "MIT",
    "type": "project",
    "description": "Design Patterns Demos in PHP",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2.8",
        "symfony/phpunit-bridge": "^4.2"
    },
    "require-dev": {
        "ext-json": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin"
    }
}
